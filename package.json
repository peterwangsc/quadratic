  },
  "description": "Quadratic HQ",
  "repository": {
    "type": "git",
    "url": "https://github.com/quadratichq/quadratic.git"
  },
  "private": false,
  "dependencies": {
    "@auth0/auth0-react": "^1.11.0",
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@monaco-editor/react": "^4.3.1",
    "@mui/icons-material": "^5.2.0",
    "@mui/material": "^5.2.2",
    "@sentry/react": "^6.19.6",
    "@sentry/tracing": "^6.19.6",
    "@szhsin/react-menu": "^2.3.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/color": "^3.0.3",
    "@types/fontfaceobserver": "^2.1.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.33",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^17.0.10",
    "ace-builds": "^1.4.13",
    "axios": "^0.24.0",
    "color": "^4.2.3",
    "date-fns": "^2.28.0",
    "electron-is-dev": "^2.0.0",
    "electron-updater": "^5.3.0",
    "fontfaceobserver": "^2.3.0",
    "fuzzysort": "^2.0.4",
    "localforage": "^1.10.0",
    "lodash.debounce": "^4.0.8",
    "monaco-editor": "^0.31.1",
    "pixi-viewport": "^4.37.0",
    "pixi.js": "^6.5.1",
    "react": "^17.0.2",
    "react-color": "^2.19.3",
    "react-device-detect": "^2.2.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.1",
    "recoil": "^0.6.0",
    "sass": "^1.56.2",
    "typescript": "^4.4.4",
    "update-electron-app": "^2.0.1",
    "web-vitals": "^1.0.1"
  },
  "main": "public/electron.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test:all": "concurrently \"npm run test:unit:ci\" \"npm run test:e2e\" \"npm run test:python\"",
    "test:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:unit:dev": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watchAll",
    "test:unit:ci": "node --experimental-vm-modules node_modules/jest/bin/jest.js --ci",
    "test:e2e": "playwright test --workers=1",
    "test:e2e:debug": "playwright test --debug",
    "test:python": "python3 tests-python/test.py",
    "test": "",
    "prettier": "prettier --write src",
    "eject": "react-scripts eject",
    "dev": "concurrently \"BROWSER=none npm start\" \"electron .\"",
    "electron": "electron .",
    "electron:build": "electron-builder build --mac --win --linux --publish never",
    "electron:publish": "electron-builder build --mac --win --linux --publish always"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "eslintIgnore": [
    "node_modules/*"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.22.2",
    "@types/lodash.debounce": "^4.0.7",
    "concurrently": "^6.3.0",
    "electron": "^15.3.0",
    "electron-builder": "^23.6.0",
    "fs-extra": "^11.1.0",
    "msdf-bmfont-xml": "^2.7.0",
    "prettier": "^2.8.0",
    "pyodide": "^0.22.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "wait-on": "^6.0.1"
  },
  "build": {
    "appId": "com.quadratichq.quadratic",
    "mac": {
      "category": "public.app-category.productivity"
    }
  },
  "files": [
    "*.js",
    "build",
    "node_modules"
  ]
}
